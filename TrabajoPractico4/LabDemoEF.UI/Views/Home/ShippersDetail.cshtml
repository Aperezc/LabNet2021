@model LabDemoEF.UI.Models.Shippers.ShippersDetail

@{
    ViewBag.Title = "ShippersDetail";
}

<h2>ShippersDetail</h2>

@using (Html.BeginForm("AddShippers", "Home", FormMethod.Post))
{
    <table class="table">
        <tbody>
            <tr>
                <td>@Html.DisplayNameFor(m => m.CompanyName)</td>
                <td>@Html.EditorFor(m => m.CompanyName)</td>
                <td>@Html.DisplayNameFor(m => m.Phone)</td>
                <td>@Html.EditorFor(m => m.Phone)</td>                
            </tr>          
        </tbody>
    </table>
    <input type="submit" value="Agregar" />
}


